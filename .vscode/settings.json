{
    "cSpell.language": "en-GB",
    "cSpell.words": [
        "ESNEXT",
        "downlevel",
        "linebreak",
        "sourcemap",
        "tslib",
        "typechecking",
        "vars"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "cSpell.ignoreWords": [
        "linebyline",
        "somefile"
    ]
}